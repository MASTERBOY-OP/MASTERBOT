

_get_repolink () {
    local regex
    regex='(https?)://github.com/.+/.+'
    if [[ $MASTER_REPO == "MASTERB0T" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL0RldmlsYm95MC9NQVNURVJCMFQvYXJjaGl2ZS9tYXN0ZXIuemlw" | base64 -d
    elif [[ $MASTER_REPO == "MASTERB0T" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL0RldmlsYm95MC9NQVNURVJCMFQvYXJjaGl2ZS9tYXN0ZXIuemlw" | base64 -d
    elif [[ $MASTER_REPO =~ $regex ]]
    then
        if [[ $MASTER_REPO_BRANCH ]]
        then
            echo "${MASTER_REPO}/archive/${MASTER_REPO_BRANCH}.zip"
        else
            echo "${MASTER_REPO}/archive/master.zip"
        fi
    else
        echo "aHR0cHM6Ly9naXRodWIuY29tL0RldmlsYm95MC9NQVNURVJCMFQvYXJjaGl2ZS9tYXN0ZXIuemlw" | base64 -d
    fi
}


_setting_bot () {
    local zippath
    zippath="MASTERB0T.zip"
    echo "  Downloading â˜…ğ•â€Œğ•’â€ŒÑ•á´›Îµğ•£â€Œğ•“â€Œğ• â€Œğ•¥â€Œê—„âº V1.0 Source Code..."
    wget -q $(_get_repolink) -O "$zippath"
    echo "  Unpacking Data ..."
    MASTERDPATH=$(zipinfo -1 "$zippath" | grep -v "/.");
    unzip -qq "$zippath"
    echo "Done"
    echo "  â˜…ğ•â€Œğ•’â€ŒÑ•á´›Îµğ•£â€Œğ•“â€Œğ• â€Œğ•¥â€Œê—„âº V1.0 Cleaning.."
    rm -rf "$zippath"
    sleep 5
    cd $MASTERPATH
    echo "    âšœï¸Starting â˜…ğ•â€Œğ•’â€ŒÑ•á´›Îµğ•£â€Œğ•“â€Œğ• â€Œğ•¥â€Œê—„âºâšœï¸     "
    echo "
    âš¡âš¡âš¡âš¡âš¡âš¡âš¡âš¡âš¡
    "

    python3 ../setup/updater.py ../requirements.txt requirements.txt
    python3 -m userbot
}

_setting_bot
